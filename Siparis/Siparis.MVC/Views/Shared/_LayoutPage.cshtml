<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Taşkıran Kantincilik : @Page.Title</title>
    <!--    favicon-->
    <link rel="shortcut icon" href="image/favicon-icon.jpg">
    <!--    font-->
    <link href='https://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Great+Vibes' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Poppins:400,500,600,700' rel='stylesheet' type='text/css'>
    <!-- Bootstrap css -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.css">
    <link rel="stylesheet" href="~/Content/css/bootstrap-theme.min.css">
    <link rel="stylesheet" href="~/Content/vendors/bootstrap-timepicker/timepicker.css">
    <link rel="stylesheet" href="~/Content/vendors/bootstrap-datepicker/css/datepicker.css">
    <link rel="stylesheet" href="~/Content/vendors/jqueryui/css/jquery-ui.css">
    <!--   font-awesome-->
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css">

    <!--    flex-CSS-->
    <link rel="stylesheet" href="~/Content/vendors/flex-slider/flexslider.css">
    <link rel="stylesheet" href="~/Content/css/animate.min.css">
    <!--    owl-carousel-->
    <link rel="stylesheet" href="~/Content/vendors/owlcarousel/owl.carousel.css">

    <!-- Custom css -->
    <link rel="stylesheet" href="~/Content/css/style.css">

    <!--    responsive css-->
    <link rel="stylesheet" href="~/Content/css/responsive.css">
    @RenderSection("styles", required: false)
</head>
<body>
    <div id="page-wrapper">
        <header class="menu-below-logo">
            <div class="container">
                <div class="logo">
                    <a href="#" id="logo" title="Caferesto" class="logo-image">
                        <img src="~/Content/image/logo.png" alt="">
                    </a>
                </div>
            </div>
            <nav class="navbar navbar-default " id="main_navbar">
                <div class="container">
                    <div class="menu-container">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                                <span class="sr-only">Menu</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav menu">
                                <li class="active"><a href="@Url.Action("Index","Home")">Anasayfa</a></li>
                                <li><a href="@Url.Action("Index","About")">Hakkımızda</a></li>
                                <li><a href="reservation.htm">Online Rezervasyon</a></li>
                                <li><a href="@Url.Action("Index","Events")">Etkinlikler</a></li>
                                <li><a href="@Url.Action("Index","Blog")">Blog</a></li>
                                <li><a href="@Url.Action("Index","Contact")">İletişim</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </nav>
        </header>
        @RenderBody()
    </div>
    <!--    start newsletter-area-->
    <section class="newsletter-area">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-md-offset-2">
                    <div class="text">
                        <h2>GET OUR NEWSLETTER</h2>
                        <p>Nam at vehicula lacus, ut consectetur</p>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="subcribs">
                        <div class="input-group">
                            <input type="text" class="form-control" placeholder="Enter Your Email Address">
                            <i class="fa fa-paper-plane-o" aria-hidden="true"></i>
                            <div class="input-group-addon">
                                <button type="submit" class="btn btn-default">SUBCRIBE</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--    End newsletter-area-->
    <!--    Start footer area-->
    <footer class="row footer-area">
        <div class="container">
            <div class="logo">
                <img src="~/Content/image/logo.png" alt="">
            </div>
            <div class="footer">
                <div class="footer-top">
                    <h3>
                        Santa Monica BoulevardLos Angeles, California 90025-4718<br>
                        <a href="#">0(123) 456 78 91</a><br>
                        contact@caferesto.com
                    </h3>
                    <ul class="social-nav">
                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="#"><i class="fa fa-google-plus" aria-hidden="true"></i></a></li>
                        <li><a href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
                        <li><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
                    </ul>
                </div>
                <div class="footer-bottom">
                    Design by <a href="#">TavillaThemes.</a> Copyright © 2016 Caferesto. All Right Reserved
                </div>
            </div>
        </div>
    </footer>
    <!--    End footer area-->
    <script src="~/Content/js/jquery-2.2.0.min.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/vendors/bootstrap-timepicker/bootstrap-timepicker.js"></script> <!--Bootstrap JS-->
    <script src="~/Content/vendors/bootstrap-datepicker/js/bootstrap-datepicker.js"></script> <!--Bootstrap JS-->
    <script src="~/Content/vendors/jqueryui/js/jquery-ui.min.js"></script>
    <!--    Flex-js-->
    <script src="~/Content/vendors/flex-slider/jquery.flexslider-min.js"></script>
    <!--    isotope JS-->
    <script src="~/Content/vendors/isotope/isotope.min.js"></script>
    <script src="~/Content/vendors/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="~/Content/vendors/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/Content/js/custom.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
